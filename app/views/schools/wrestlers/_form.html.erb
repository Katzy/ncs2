    <div class="ui form">
    <%= form_for [@wrestler]  do |f| %>
      <% if @wrestler.errors.any? %>
        <div id="error_explanation">
          <h2><%= pluralize(@wrestler.errors.count, "error") %> prohibited this user from being saved:</h2>

