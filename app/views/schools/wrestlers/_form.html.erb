        </div>
        
        <div class="two wide field">
          <%= f.label :grade, "Grade" %><br>
          <%= f.collection_select :grade, ['9', '10', '11', '12'], :to_s, :to_s, :include_blank => true %>
        </div>

        <div class="four wide field">
